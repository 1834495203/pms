<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.auth.mapper.VisitorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.auth.po.Visitor">
        <id column="vid" property="vid" />
        <result column="name" property="name" />
        <result column="required_doorplate" property="requiredDoorplate" />
        <result column="required_time" property="requiredTime" />
        <result column="state" property="state" />
        <result column="license_plate" property="licensePlate" />
        <result column="gender" property="gender" />
        <result column="leaving_time" property="leavingTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        vid, name, required_doorplate, required_time, state, license_plate, gender, leaving_time
    </sql>

</mapper>
