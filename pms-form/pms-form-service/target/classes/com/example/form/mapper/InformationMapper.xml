<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.form.mapper.InformationMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.form.model.po.Information">
        <id column="iid" property="iid" />
        <result column="area" property="area" />
        <result column="hid" property="hid" />
        <result column="state" property="state" />
        <result column="pid" property="pid" />
        <result column="doorplate" property="doorplate"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        iid, area, hid, state, pid, doorplate
    </sql>

</mapper>
